# terraform.tfvars
# 프로젝트 전역 변수 값 설정

# ==================================================
# 기본 설정
# ==================================================
aws_region = "ap-northeast-2"  # 서울 리전
prefix     = "etech-hatiolab"         # 리소스 이름 접두사

# ==================================================
# EKS 클러스터 설정
# ==================================================
kubernetes_version = "1.32"       # Kubernetes 버전 고정
node_instance_type = "t3.medium"  # 워커 노드 인스턴스 타입